{
    "name": "paneedesign/database-swiftmailer-bundle",
    "description": "Database Spool Bundle for SwiftMailer and Symfony 3+",
    "type": "library",
    "keywords": ["swiftmailer", "database", "ped", "symfony", "bundle"],
    "homepage": "https://github.com/paneedesign/DatabaseSwiftMailerBundle",
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Amorim",
            "email": "email@rafaelamorim.me",
            "homepage": "http://rafaelamorim.me",
            "role": "Developer"
        },
        {
            "name": "Luigi Cardamone",
            "email": "luigi.cardamone@ped.technology",
            "role": "Senior Back-end Developer"
        },
        {
            "name": "Fabiano Roberto",
            "email": "fabiano.roberto@ped.technology",
            "role": "Senior Back-end Developer"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "symfony/framework-bundle": "^4.2|^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "symfony/form": "^4.2|^5.0",
        "symfony/swiftmailer-bundle": "^3.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PaneeDesign\\DatabaseSwiftMailerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaneeDesign\\DatabaseSwiftMailerBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "beautify": [
            "vendor/bin/php-cs-fixer fix --verbose --config=.php_cs.dist src"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
